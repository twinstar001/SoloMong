<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean	id="testControler"
			class="com.solomong.testController">
	</bean>
	
	<bean	id="MemberController"
			class="com.solomong.member.controller.MemberController">
			
	</bean>
	
	<bean	id="RefigeratorController"
			class="com.solomong.refrigerator.controller.RefrigeratorController">
			
	</bean>
	
	<bean	id="RecipeController"
			class="com.solomong.www.recipe.web.RecipeController">
			<property name="recipeService" ref="recipeService" />
	</bean>
	
	<bean id="CommunityBoardController" class="com.solomong.board.web.CommunityBoardController">
		<property name="communityBoardService" ref="communityBoardService" />
	</bean>
	

</beans>
