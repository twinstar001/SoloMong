<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean id="communityDaoOracle" class="com.solomong.community.dao.CommunityDaoImplForOracle">
		<property name="sqlSessionTemplate" ref="sqlSessionTemplate"></property>
	</bean>
	
	<bean id="communityService" class="com.solomong.community.service.CommunityServiceImpl">
		<property name="communityDao" ref="communityDaoOracle"></property>
	</bean>
</beans>
